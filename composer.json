{
  "name": "altgen/ai-alt-text-generator",
  "description": "WordPress plugin that generates image alt text using AI APIs.",
  "type": "wordpress-plugin",
  "license": "GPL-2.0-or-later",
  "require": {
    "php": ">=7.4",
    "google-gemini-php/client": "^2.5",
    "composer/installers": "^2.2",
    "openai-php/client": "^0.10.3",
    "symfony/http-client": "^6.4",
    "nyholm/psr7": "^1.8",
    "claude-php/claude-3-api": "^0.1.23"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.5"
  },
  "autoload": {
    "psr-4": {
      "AiAltText\\": "includes/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "AiAltText\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "support": {
    "issues": "https://github.com/altgen/ai-alt-text-generator/issues"
  },
  "config": {
    "allow-plugins": {
      "composer/installers": true,
      "php-http/discovery": true
    }
  }
}
